{
  "name": "thangntt-backend-boilerplate",
  "version": "0.1.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon -w src --exec \"node src --presets es2016,stage-0\"",
    "build": "cross-env NODE_ENV=production babel src -s -D -d dist --presets es2016,stage-0",
    "start": "cross-env NODE_ENV=production forever start dist/index.js",
    "edit": "swagger_swagger_fileName=src/api/swagger/swagger.yaml swagger project edit",
    "prestart": "npm run -s build",
    "test": "eslint src",
    "migrate:create": "sequelize migration:create --migrations-path ./src/migrations --config ./src/conf/db_config.json",
    "migrate:latest": "sequelize db:migrate --migrations-path ./src/migrations --config ./src/conf/db_config.json"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/developit/express-es6-rest-api.git"
  },
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "crypto": "^1.0.1",
    "express": "^4.13.3",
    "momentjs": "^2.0.0",
    "morgan": "^1.8.0",
    "multer": "^1.3.0",
    "mysql2": "^1.4.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "resource-router-middleware": "^0.6.0",
    "sequelize": "^4.5.0",
    "swagger-express-middleware": "^1.0.0-alpha.12",
    "swagger-ui-express": "^2.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^5.0.5",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2",
    "sequelize-cli": "^2.8.0",
    "standard": "^10.0.3"
  },
  "bugs": {
    "url": "https://github.com/developit/express-es6-rest-api/issues"
  },
  "homepage": "https://github.com/developit/express-es6-rest-api#readme",
  "keywords": [
    "thangntt",
    "boilerplate",
    "express",
    "babel",
    "es6"
  ]
}
